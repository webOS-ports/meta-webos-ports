From 3f6051aeaf3e47823a2478e4c1e90c97e039e21e Mon Sep 17 00:00:00 2001
From: Christophe Chapuis <chris.chapuis@gmail.com>
Date: Sat, 5 May 2018 13:50:53 +0000
Subject: [PATCH 1/2] Fix invalid method overload

---
 external/android-emugl/host/libs/Translator/EGL/EglGlobalInfo.cpp | 2 +-
 external/android-emugl/host/libs/Translator/EGL/EglGlobalInfo.h   | 2 +-
 external/android-emugl/host/libs/Translator/EGL/EglImp.cpp        | 2 +-
 3 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/external/android-emugl/host/libs/Translator/EGL/EglGlobalInfo.cpp b/external/android-emugl/host/libs/Translator/EGL/EglGlobalInfo.cpp
index 95bbcda..ef2d44e 100644
--- a/external/android-emugl/host/libs/Translator/EGL/EglGlobalInfo.cpp
+++ b/external/android-emugl/host/libs/Translator/EGL/EglGlobalInfo.cpp
@@ -84,7 +84,7 @@ bool  EglGlobalInfo::removeDisplay(EGLDisplay dpy) {
     return false;
 }
 
-EglDisplay* EglGlobalInfo::getDisplay(EGLNativeDisplayType dpy) const {
+EglDisplay* EglGlobalInfo::getDisplayFromNative(EGLNativeDisplayType dpy) const {
     emugl::Mutex::AutoLock mutex(m_lock);
     for (size_t n = 0; n < m_displays.size(); ++n) {
         if (m_displays[n]->getNativeDisplay() == dpy) {
diff --git a/external/android-emugl/host/libs/Translator/EGL/EglGlobalInfo.h b/external/android-emugl/host/libs/Translator/EGL/EglGlobalInfo.h
index 8fa8acc..0a386a0 100644
--- a/external/android-emugl/host/libs/Translator/EGL/EglGlobalInfo.h
+++ b/external/android-emugl/host/libs/Translator/EGL/EglGlobalInfo.h
@@ -55,7 +55,7 @@ public:
 
     // Return the EglDisplay instance corresponding to a given native |dpy|
     // value.
-    EglDisplay* getDisplay(EGLNativeDisplayType dpy) const;
+    EglDisplay* getDisplayFromNative(EGLNativeDisplayType dpy) const;
 
     // Return the EglDisplay instance corresponding to a given EGLDisplay |dpy|
     // value. NULL if none matches.
diff --git a/external/android-emugl/host/libs/Translator/EGL/EglImp.cpp b/external/android-emugl/host/libs/Translator/EGL/EglImp.cpp
index df38472..c3b827f 100644
--- a/external/android-emugl/host/libs/Translator/EGL/EglImp.cpp
+++ b/external/android-emugl/host/libs/Translator/EGL/EglImp.cpp
@@ -177,7 +177,7 @@ EGLAPI EGLDisplay EGLAPIENTRY eglGetDisplay(EGLNativeDisplayType display_id) {
 
     initGlobalInfo();
 
-    if ((dpy = g_eglInfo->getDisplay(display_id))) {
+    if ((dpy = g_eglInfo->getDisplayFromNative(display_id))) {
         return dpy;
     }
     if (display_id != EGL_DEFAULT_DISPLAY) {

From 1fe40b1e71ad1b60ee4d8edfce1bd798be963fc6 Mon Sep 17 00:00:00 2001
From: Christophe Chapuis <chris.chapuis@gmail.com>
Date: Wed, 22 May 2024 20:29:51 +0000
Subject: [PATCH] Remove webos_machine_dep

Upstream-Status: Pending

Signed-off-by: Christophe Chapuis <chris.chapuis@gmail.com>
---
 common/CMakeLists.txt | 2 --
 imap/CMakeLists.txt   | 2 --
 pop/CMakeLists.txt    | 2 --
 smtp/CMakeLists.txt   | 2 --
 4 files changed, 8 deletions(-)

diff --git a/common/CMakeLists.txt b/common/CMakeLists.txt
index aefe603..817b42f 100644
--- a/common/CMakeLists.txt
+++ b/common/CMakeLists.txt
@@ -27,8 +27,6 @@ webos_component(2 0 0)
 webos_add_compiler_flags(ALL "-DMOJ_LINUX")
 webos_add_compiler_flags(ALL "-O2 -ggdb -Wuninitialized -Winit-self -Wno-error=strict-aliasing")
 
-webos_machine_dep()
-
 # These variables can change the default behaviour and need to be set before calling find_package
 set(Boost_USE_STATIC_LIBS OFF)
 set(Boost_USE_STATIC_RUNTIME OFF)
diff --git a/imap/CMakeLists.txt b/imap/CMakeLists.txt
index e7d543b..1aaf6c6 100644
--- a/imap/CMakeLists.txt
+++ b/imap/CMakeLists.txt
@@ -29,8 +29,6 @@ webos_add_compiler_flags(ALL "-O -ggdb -Wuninitialized -Winit-self")
 
 webos_add_linker_options(ALL "--unresolved-symbols=ignore-in-shared-libs -rpath-link ${WEBOS_INSTALL_ROOT}/lib")
 
-webos_machine_dep()
-
 include_directories(inc)
 
 # These variables can change the default behaviour and need to be set before calling find_package

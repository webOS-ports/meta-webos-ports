+[Unit]
+Description=webos - "%n"
+Requires=ls-hubd.service
+After=ls-hubd.service
+
+[Service]
+Type=simple
+OOMScoreAdjust=-500
+EnvironmentFile=-/var/systemd/system/env/appinstalld.env
+Environment=CHARSET=UTF-8
+ExecStart=/usr/sbin/appinstalld
+Restart=on-failure
+
+[Install]
+WantedBy=multi-user.target

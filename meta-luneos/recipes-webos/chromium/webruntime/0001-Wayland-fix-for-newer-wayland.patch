From 24d65f6c505fd3b3d80338b8401ed20409517176 Mon Sep 17 00:00:00 2001
From: Herman van Hazendonk <github.com@herrie.org>
Date: Thu, 18 Jun 2020 12:00:12 +0200
Subject: [PATCH] screen.cc: Fix for newer Wayland

In order to work with our newer Wayland version.

Signed-off-by: Herman van Hazendonk <github.com@herrie.org>
---
 src/ozone/wayland/screen.cc | 7 ++++++-
 1 file changed, 6 insertions(+), 1 deletion(-)

diff --git a/src/ozone/wayland/screen.cc b/src/ozone/wayland/screen.cc
index 7209f7cdb..9f9377dbe 100644
--- a/src/ozone/wayland/screen.cc
+++ b/src/ozone/wayland/screen.cc
@@ -35,6 +35,11 @@ const int k180DegreesCounterClockwise = 180;
 const int k270DegreesCounterClockwise = 270;
 }  // namespace
 
+static void
+display_handle_scale(void *data, struct wl_output *output, int32_t scale)
+{
+}
+
 WaylandScreen::WaylandScreen(wl_registry* registry, uint32_t id)
     : output_(nullptr),
 #if defined(OS_WEBOS)
@@ -46,7 +51,7 @@ WaylandScreen::WaylandScreen(wl_registry* registry, uint32_t id)
     WaylandScreen::OutputHandleGeometry,
     WaylandScreen::OutputHandleMode,
 #if defined(OS_WEBOS)
-    WaylandScreen::OutputDone,
+    WaylandScreen::OutputDone, display_handle_scale,
 #endif
   };
 
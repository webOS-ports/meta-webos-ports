From fc073b5d8e803c0df3cf1c5c57e9a52493467464 Mon Sep 17 00:00:00 2001
From: Herrie <Github.com@herrie.org>
Date: Thu, 19 Oct 2023 20:13:09 +0200
Subject: [PATCH] defaultPreferences.txt: Customize ringtone and wallpaper for
 LuneOS

We use a custom ringtone and wallpaper for LuneOS.

Signed-off-by: Herman van Hazendonk <github.com@herrie.org>
---
Upstream-Status: Inappropriate [LuneOS specific]

 files/conf/defaultPreferences.txt | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/files/conf/defaultPreferences.txt b/files/conf/defaultPreferences.txt
index 45a28f9..75521c5 100644
--- a/files/conf/defaultPreferences.txt
+++ b/files/conf/defaultPreferences.txt
@@ -15,10 +15,10 @@
 		"timeFormat":			"HH12",
 		"locale":				{ "languageCode":"en", "countryCode":"us", "phoneRegion": { "countryName":"United States", "countryCode":"us"} },
 		"region":				{ "countryName":"United States", "countryCode":"us"},
-		"ringtone":				{ "fullPath":"/usr/lib/luna/customization/copy_binaries/media/internal/ringtones/Pre.mp3" , "name":"PrÄ“" },
+		"ringtone":				{ "fullPath":"/usr/palm/sounds/ringtone.mp3" , "name":"ringtone.mp3" },
 		"alerttone":			{ "fullPath":"/usr/palm/sounds/alert.wav" , "name":"alert.wav" },
 		"notificationtone":		{ "fullPath":"/usr/palm/sounds/notification.wav" , "name":"notification.wav" },
-		"wallpaper":			{ "wallpaperName":"bluerocks.png" , "wallpaperFile":"/usr/lib/luna/system/luna-systemui/images/bluerocks.png" , "wallpaperThumbFile":""},
+		"wallpaper":			{ "wallpaperName":"LuneOS.jpg" , "wallpaperFile":"/usr/share/wallpapers/LuneOS.jpg" , "wallpaperThumbFile":""},
 		"airplaneMode":			false,
 		"showAlertsWhenLocked":	true,
 		"LEDThrobberEnabled":	true,

+++ b/chromium/third_party/WebKit/public/web/WindowFeaturesStructTraits.cpp
@@ -27,7 +27,10 @@ bool StructTraits<::blink::mojom::WindowFeaturesDataView,
   out->resizable = data.resizable();
   out->fullscreen = data.fullscreen();
   out->dialog = data.dialog();
-  out->additionalFeatures = data.additionalFeatures();
+  
+  std::vector<base::string16> additionalFeatures;
+  data.ReadAdditionalfeatures(&additionalFeatures);
+  out->additionalFeatures = additionalFeatures;
   return true;
 }
 

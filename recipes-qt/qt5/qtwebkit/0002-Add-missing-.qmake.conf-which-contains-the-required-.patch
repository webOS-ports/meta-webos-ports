From af73a851106cfa55ad7aac89d7b22a272647502f Mon Sep 17 00:00:00 2001
From: Simon Busch <morphis@gravedo.de>
Date: Tue, 27 Aug 2013 12:42:39 +0200
Subject: [PATCH 2/2] Add missing .qmake.conf which contains the required
 MODULE_VERSION

Signed-off-by: Simon Busch <morphis@gravedo.de>
---
 .qmake.conf | 6 ++++++
 1 file changed, 6 insertions(+)
 create mode 100644 .qmake.conf

diff --git a/.qmake.conf b/.qmake.conf
new file mode 100644
index 0000000..dde95ab
--- /dev/null
+++ b/.qmake.conf
@@ -0,0 +1,6 @@
+# The qmake generated module files belong into our Tools/qmake dir
+MODULE_QMAKE_OUTDIR = $$shadowed($$PWD/Tools/qmake)
+QMAKEPATH += $$PWD/Tools/qmake $$MODULE_QMAKE_OUTDIR
+load(qt_build_config)
+
+MODULE_VERSION = 5.1.2
